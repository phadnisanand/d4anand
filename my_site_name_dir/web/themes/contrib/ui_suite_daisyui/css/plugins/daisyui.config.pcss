/**
 * Import DaisyUI excluding DaisyUI reset.css as Tailwind already provide same mechanism (Preflight).
 * We exclude DaisyUI reset.css as it prevents style from base layer to be applied.
 */
@plugin "daisyui" {
  exclude: reset;
  themes: light --default, dark, cupcake, aqua, black, bumblebee, cmyk, corporate, cyberpunk, dracula, emerald, fantasy, forest, garden, halloween, lofi, luxury, pastel, retro, synthwave, valentine, wireframe, autumn, business, acid, lemonade, night, coffee, winter, dim, nord, sunset, caramellatte, abyss, silk;
  logs: false;
}
